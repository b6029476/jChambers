<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Event name</th>
      <th>Event type</th>
      <th>Location</th>
      <th>City</th>
      <th>Tickets</th>
      <th>Event date</th>
      <th>Event time</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.ename %></td>
        <td><%= event.etype %></td>
        <td><%= event.location %></td>
        <td><%= event.city %></td>
        <td><%= event.tickets %></td>
        <td><%= event.edate %></td>
        <td><%= event.etime %></td>
        <td><%= event.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
