<div class="container">
	<h2> please login</h2>

	<%= form_tag sessions_path do %>

	<%= text_field_tag :userid, nil, placeholder: 'enter username', class: 'form-control' %>
	<%= password_field_tag :password, nil, placeholder: 'enter password', class: 'form-control' %>
	<%= submit_tag ' Log in' %>
	<% end %>
</div>
